---
import { getEventsPerYear } from "@/lib/api"
import type { Events } from "@/lib/api/types.ts"
import "@/styles/globals.css"
import Layout from '@/layouts/Layout.astro'
import { t } from 'astro-i18n'

const events: Events = await getEventsPerYear(2024).catch(
	(_error: unknown) => {
		return {} as Events
	},
)
---

<Layout title={t("index.title")}>
    <main>
        <p>{t("index.hello")}</p>
        <p>{JSON.stringify(events)}</p>
    </main>
</Layout>
