---
import Layout from "@/layouts/Layout.astro"
import { getEventsPerYear, isAlive } from "@/lib/api"
import type { ErrorResponse, Events } from "@/lib/api/types.ts"
import "@/styles/globals.css"
import { astroI18n, t } from "astro-i18n"

console.log(isAlive())

const events: Events | ErrorResponse = await getEventsPerYear(2024)
---

<Layout title={t("index.title")}>
    <main>
        <p>{t("index.hello")}</p>
        <p>{JSON.stringify(events)}</p>
    </main>
</Layout>
